---
export interface Props {
  title?: string;
}

const { title = 'Southeast Jiu-Jitsu Schedule' } = Astro.props;
---

<iframe 
	src="https://southeastjiujitsu.wod.guru/zajecia?hideTop=1"
	style="width: 100%; height: calc(100vh - 75px); border: 0;"
	title={title}
	allow="fullscreen"
	sandbox="allow-scripts allow-same-origin allow-forms allow-popups allow-top-navigation"
></iframe>